<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_setting_notes"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="lmu.de.unificiencyandroid.components.setting.SettingNotes">

    <TextView
        android:id="@+id/settingnote_meinnote"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Meine Notes:"
        android:textSize="40dp"
        android:gravity="center"
        android:background="#009440"
        />

    <ListView
        android:id="@+id/settingnote_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/settingnote_meinnote"
        ></ListView>

</RelativeLayout>
